  function ima_ck(nom) {
    var aux = imagidx;
    imagidx = nom;
    ima_of(nom);
    return true;
  };

  function ima_on(nom) {
    if (verok == 1) {
      imgOn = eval("menu_" + nom + "_on.src");
      document[nom].src = imgOn;
      return true;
    };
  };

  function ima_of(nom) {
    if ((verok == 1) && (imagidx != nom)) {
      imgOn = eval("menu_" + nom + "_of.src");
      document[nom].src = imgOn;
      return true;
    };
  };

